{
  "name": "hotel_reservation",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc --",
    "watch": "yarn run build -- -w",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart ",
    "testWithCoverage": "jest --updateSnapshot --watchAll --coverage && open ./coverage/lcov-report/index.html",
    "gql-schema-download": "apollo schema:download --endpoint=https://us1.prisma.sh/public-luckox-377/reservation-graphql-backend/dev",
    "gql-codegen": "apollo client:codegen --queries=./src/graphql/**/*.ts --localSchemaFile=./schema.json --target=ts-modern"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "apollo-boost": "^0.1.28",
    "graphql": "^14.1.1",
    "native-base": "^2.12.1",
    "react": "16.6.3",
    "react-apollo": "^2.4.1",
    "react-native": "0.58.5",
    "react-native-awesome-alerts": "^1.2.0",
    "react-native-datepicker-dialog": "^0.0.9",
    "react-native-gesture-handler": "^1.0.16",
    "react-native-responsive-fontsize": "^0.1.8",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.3.0",
    "waait": "^1.0.4"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.6",
    "@types/jsdom": "^12.2.3",
    "@types/react": "16.7.0",
    "@types/react-dom": "^16.8.2",
    "@types/react-native": "0.55.2",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^3.0.3",
    "@types/react-test-renderer": "16.0.1",
    "apollo": "^2.5.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "enzyme-to-json": "^3.3.5",
    "graphql-code-generator": "^0.17.0",
    "jest": "24.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-fetch-mock": "^2.1.1",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "0.52.0",
    "react-dom": "^16.8.3",
    "react-test-renderer": "16.6.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "preset": "react-native"
  }
}
